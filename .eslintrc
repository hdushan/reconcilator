{
    "extends": ["airbnb-base", "prettier"],
    "plugins": ["prettier"],
    "env": {
      "node": true
    },
    "rules": {
        "semi": [2, "never"],
        "import/prefer-default-export": "off",
        "object-curly-newline": "off",
        "prettier/prettier": "error",
        "operator-linebreak": [
          "error",
          "after",
          {
            "overrides": {
              ":": "before"
            }
          }
        ]
    }
  }